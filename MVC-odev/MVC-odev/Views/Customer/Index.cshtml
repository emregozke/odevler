
@model List<MVC_odev.Models.CustomerModel>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>CustomerId</th>
            <th>CompanyName</th>
            <th>ContactName</th>
            <th>ContactTitle</th>
            <th>Adress</th>
            <th>City</th>
        </tr>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.CustomerId</td>
                <td>@item.CompanyName</td>
                <td>@item.ContactName</td>
                <td>@item.ContactTitle</td>
                <td>@item.Adress</td>
                <td>@item.City</td>

            </tr>
            
        }
    </table>
</body>
</html>
